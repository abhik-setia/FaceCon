{
  "manifest_version": 2,
  "name": "FaceCon",
  "version": "1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["JS/jquery-3.2.1.min.js","content.js"]
  }
],
  "icons":{
    "16": "Icons/icon.png",
  "64":"Icons/icon_64.png",
  "128":"Icons/icon_128.png"
},
  "browser_action": {
  "default_icon":"Icons/icon.png",
  "default_popup":"popup.html",
  "default_title":"FaceCon is live 8)"
},
"background": {
  "scripts": ["JS/jquery-3.2.1.min.js","background.js"]
},
"permissions":[
  "storage",
  "tabs",
  "activeTab",
 "https://twitter.com/*"
]
}