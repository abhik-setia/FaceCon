{
  "manifest_version": 2,
  "name": "FaceCon",
  "version": "1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.2.1.min.js","content.js"]
  }
],
  "browser_action": {
  "default_icon": "Icons/icon_64.png",
  "default_popup":"popup.html",
  "default_title":"Click Here!"
},
"background": {
  "scripts": ["background.js"]
}
}